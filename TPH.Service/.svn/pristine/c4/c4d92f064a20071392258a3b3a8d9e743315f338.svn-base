package com.artica.telesalud.tph.lightweightmodel;

import java.io.Serializable;
import java.util.ArrayList;
import java.util.Date;

import com.artica.telesalud.client.shared.ValuedItem;

public class NotaEvolucion implements Serializable{
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 2188629564603919071L;
	private Date fecha;
	private ValuedItem<Integer, String> dxPrincipal;
	private ArrayList<ValuedItem<Integer, String>> dxSecundarios;
	private String recomendaciones;
	private ValuedItem<Integer, String> medicoTratante;
	
	
	public Date getFecha() {
		return fecha;
	}
	public void setFecha(Date fecha) {
		this.fecha = fecha;
	}
	public ValuedItem<Integer, String> getDxPrincipal() {
		return dxPrincipal;
	}
	public void setDxPrincipal(ValuedItem<Integer, String> dxPrincipal) {
		this.dxPrincipal = dxPrincipal;
	}
	public ArrayList<ValuedItem<Integer, String>> getDxSecundarios() {
		return dxSecundarios;
	}
	public void setDxSecundarios(ArrayList<ValuedItem<Integer, String>> dxSecundarios) {
		this.dxSecundarios = dxSecundarios;
	}
	public String getRecomendaciones() {
		return recomendaciones;
	}
	public void setRecomendaciones(String recomendaciones) {
		this.recomendaciones = recomendaciones;
	}
	public ValuedItem<Integer, String> getMedicoTratante() {
		return medicoTratante;
	}
	public void setMedicoTratante(ValuedItem<Integer, String> medicoTratante) {
		this.medicoTratante = medicoTratante;
	}

}
